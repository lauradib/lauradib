Algoritmo "Tabuadado8"


Var
multiplicador: inteiro
resultado: inteiro

Inicio
 multiplicador <- 1

escreval ("Digite o multiplicador:")
 leia (multiplicador)

enquanto (multiplicador <= 10) faca
 resultado <- 8 * multiplicador

 escreval ("8 vezes", multiplicador, "é igual a:")
escreva (resultado)


fimEnquanto

 Fimalgoritmo